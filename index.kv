Screen:

    NavigationLayout:

        ScreenManager:

            Screen:

                BoxLayout:
                    orientation: 'vertical'

                    MDToolbar:
                        id: toolbar
                        pos_hint: {"top": 1}
                        elevation: 10
                        title: "GraphMath"
                        left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]


                        MDIconButton:
                            id: icon
                            icon: "brightness-4"
                            pos_hint: {"center_x": .5, "center_y": .5}
                            on_press: app.change_theme_root()
                    ScreenManager:
                        id: screen_manager


                    Widget:

        MDNavigationDrawer:
            id: nav_drawer

            ContentNavigationDrawer:
                ScrollView:
                    MDList:
                        id: drawer_list
